";\nimport Header from '../Header/header';\nimport BookAdd from '../Books/BookAdd/bookAdd';\nimport BookEdit from \"../Books/BookEdit/bookEdit\";\n\n// import Login from \"../Login/login\";\n\nclass App extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            countries: [], authors: [], books: [], selectedBook: {}\n        }\n    }\n\n    render() {\n        return (<Router>\n            <Header/>\n            <main>\n                <div className=\"container\">\n                    <Route path={\"/countries\"} exact render={() => <Countries countries={this.state.countries}/>}/>\n                    <Route path={\"/authors\"} exact render={() => <Authors authors={this.state.authors}/>}/>\n                    <Route path={\"/books/add\"} exact render={() => <BookAdd countries={this.state.countries}\n                                                                            authors={this.state.authors}\n                                                                            onAddBook={this.addBook}/>}/>\n                    <Route path={\"/books/edit/:id\"} exact render={() => <BookEdit countries={this.state.countries}\n                                                                                  authors={this.state.authors}\n                                                                                  onEditBook={this.editBook}\n                                                                                  book={this.state.selectedBook}/>}/>\n                    <Route path={\"/books\"} exact render={() => <Books books={this.state.books}\n                                                                      onDelete={this.deleteBook}\n                                                                      onEdit={this.getBook}\n                                                                      onBorrowBook={this.borrowBook}/>}/>\n                    {/*<Route path={\"/login\"} exact render={() => <Login onLogin={this.fetchData}/>}/>*/}\n                    {/*<Redirect to={\"/books\"}/>*/}\n                </div>\n            </main>\n        </Router>);\n    }\n\n    componentDidMount() {\n        this.fetchData()\n    }\n\n    fetchData = () => {\n        this.loadCountries();\n        this.loadAuthors();\n        this.loadBooks();\n    }\n    loadCountries = () => {\n        LibraryService.fetchCountries()\n            .then((data) => {\n                this.setState({\n                    countries: data.data\n                })\n            });\n    }\n    loadAuthors = () => {\n        LibraryService.fetchAuthors()\n            .then((data) => {\n                this.setState({\n                    authors: data.data\n                })\n            });\n    }\n    loadBooks = () => {\n        LibraryService.fetchBooks()\n            .then((data) => {\n                this.setState({\n                    books: data.data\n                })\n            });\n    }\n\n    deleteBook = (id) => {\n        LibraryService.deleteBook(id)\n            .then(() => {\n                this.loadBooks();\n            });\n    }\n\n    addBook = (name, category, author, availableCopies) => {\n        LibraryService.addBook(name, category, author, availableCopies)\n            .then(() => {\n                this.loadBooks();\n            });\n    }\n\n    getBook = (id) => {\n        LibraryService.getBook(id)\n            .then((data) => {\n                this.setState({\n                    selectedBook: data.data\n                })\n            })\n    }\n\n    editBook = (id, name, category, author, availableCopies) => {\n        LibraryService.editBook(id, name, category, author, availableCopies)\n            .then(() => {\n                this.loadBooks();\n            });\n    }\n\n    borrowBook = (id) => {\n        LibraryService.borrowBook(id)\n            .then((data) => {\n                this.setState({\n                    selectedBook: data.data\n                })\n            })\n    }\n}\n\nexport default App;\n","D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Authors\\authors.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Header\\header.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Countries\\countries.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\repository\\libraryRepository.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Books\\BookAdd\\bookAdd.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Books\\BookEdit\\bookEdit.js",[],"D:\\IdeaProjects\\emt-lab-frontend\\src\\components\\Books\\BookList\\books.js",["55"],"import React from 'react';\r\nimport ReactPaginate from 'react-paginate'\r\nimport {Link} from 'react-router-dom';\r\n\r\nimport BookTerm from '../BookTerm/bookTerm';\r\nimport libraryRepository from \"../../../repository/libraryRepository\";\r\n\r\nclass Books extends React.Component {\r\n\r\n    constructor(props) {\r\n        super(props);\r\n\r\n        this.state = {\r\n            page: 0, size: 2\r\n        }\r\n    }\r\n\r\n    render() {\r\n        const offset = this.state.size * this.state.page;\r\n        const nextPageOffset = offset + this.state.size;\r\n        const pageCount = Math.ceil(this.props.books.length / this.state.size);\r\n        const books = this.getBooksPage(offset, nextPageOffset);\r\n        console.log(books, pageCount)\r\n\r\n        return (<div className={\"container mm-4 mt-5\"}>\r\n            <div className={\"row\"}>\r\n                <div className={\"table-responsive\"}>\r\n                    <table className={\"table table-striped\"}>\r\n                        <thead>\r\n                        <tr>\r\n                            <th scope={\"col\"}>Name</th>\r\n                            <th scope={\"col\"}>Category</th>\r\n                            <th scope={\"col\"}>Author</th>\r\n                            <th scope={\"col\"}>Available Copies</th>\r\n                        </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                        {books}\r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n                <div className=\"col mb-3\">\r\n                    <div className=\"row\">\r\n                        <div className=\"col-sm-12 col-md-12\">\r\n                            <Link className={\"btn btn-block btn-dark\"} to={\"/books/add\"}>Add new book</Link>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <ReactPaginate previousLabel={\"back\"}\r\n                           nextLabel={\"next\"}\r\n                           breakLabel={<a href=\"/#\">...</a>}\r\n                           breakClassName={\"break-me\"}\r\n                           pageClassName={\"ml-1\"}\r\n                           pageCount={pageCount}\r\n                           marginPagesDisplayed={2}\r\n                           pageRangeDisplayed={5}\r\n                           onPageChange={this.handlePageClick}\r\n                           containerClassName={\"pagination m-4 justify-content-center\"}\r\n                           activeClassName={\"active\"}/>\r\n        </div>);\r\n    }\r\n\r\n    handlePageClick = (data) => {\r\n        let selected = data.selected;\r\n        console.log(selected)\r\n        this.setState({\r\n            page: selected\r\n        })\r\n    }\r\n\r\n    getBooksPage = (offset, nextPageOffset) => {\r\n        console.log(offset, nextPageOffset)\r\n        return this.props.books.map((term, index) => {\r\n            return (<BookTerm term={term} onDelete={this.props.onDelete} onEdit={this.props.onEdit}\r\n                              onBorrowBook={this.props.onBorrowBook}/>);\r\n        }).filter((book, index) => {\r\n            return index >= offset && index < nextPageOffset;\r\n        })\r\n    }\r\n}\r\n\r\nexport default Books;","D:\\IdeaProjects\